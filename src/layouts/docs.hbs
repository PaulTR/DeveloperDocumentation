---
date: <%= new Date() %>
---
<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width">

    <title>Sphero Docs | {{title}}</title>

    <!-- jQuery -->
    <script src="{{assets}}/js/jquery-1.11.1.min.js"></script>

    <!-- Styles-->
    <link href="{{assets}}/css/style.css" rel="stylesheet">

    <!-- Let me embed vines -->
    <script async src="//platform.vine.co/static/scripts/embed.js" charset="utf-8"></script>

    <!-- Favicon -->
    <link rel="shortcut icon" href="{{assets}}/sphero.ico" type="image/x-icon" />

    <!-- Vendor theme mods -->
    <script src="{{assets}}/js/jquery-styling.js"></script>
    <script src="{{assets}}/js/prettify.js"></script>

    <!-- Sphero theme mods -->

    <!-- Magnific Popup CSS and JS files -->
    <link rel="stylesheet" href="{{assets}}/css/magnific-popup.css">
    <script src="{{assets}}/js/jquery.magnific-popup.min.js"></script>

    <!-- Meta -->
    <meta content="Sphero" property="og:title">
    <meta content="Documentation for Sphero" name="description">

  </head>
  <body role="flatdoc">
    <div id="site-wrapper">
      <div id="site-canvas">
        {{> mobile-nav}}
        <div id="docs">
          <header class="large-12 columns navigation always-show">
            <div class="left-side">
              <a href="https://www.sphero.com"><img src="{{assets}}/images/spheromark.png" id="logo"></a>
            </div>
            <div class="right-side">
              <div class="edit-button">
                <a href="https://github.com/orbotix/DeveloperDocumentation/edit/master/src/content/{{platform}}/{{basename}}.md" class="button">Edit this page</a>
              </div>
              <div class="home-button">
                <div class="ion-ios7-cart-outline"></div>
                <a href="http://store.gosphero.com">Shop</a>
              </div>
            </div>
          </header>
          <div class="content-root" data-spy="scroll" data-target="#toc">
            <div class="menubar">
              <div class="menu section" role="flatdoc-menu">
                <ul>
                  <li><a href="/sphero-robot-basics" class="level-0">Sphero Robot Basics</a></li>
                  <li><a href="/sprk-edu" class="level-0">SPRK - EDU</a></li>
                  <li><a href="/sdk" class="level-0">SDK</a></li>
                  <li><a href="/api-reference" class="level-0">API Reference</a></li>
                </ul>
              </div>
              <div class="menu section" role="flatdoc-menu">
                <ul id="toc">
                  {{#withSort pages "data.order"}}
                    {{#is data.section "Sphero Robot Basics"}}
                    <li><a href="/sphero-robot-basics/{{basename}}">{{data.title}}</a></li>
                    {{/is}}
                  {{/withSort}}
                  {{#withSort pages "data.order"}}
                    {{#is data.section "SPRK - EDU"}}
                    <li><a href="/sprk-edu/{{basename}}">{{data.title}}</a></li>
                    {{/is}}
                  {{/withSort}}
                  {{#withSort pages "data.order"}}
                    {{#is data.section "SDK Documentation"}}
                    <li><a href="/sdk/{{basename}}">{{data.title}}</a></li>
                    {{/is}}
                  {{/withSort}}
                  {{#withSort pages "data.order"}}
                    {{#is data.section "API Reference"}}
                    <li><a href="/api-reference/{{basename}}">{{data.title}}</a></li>
                    {{/is}}
                  {{/withSort}}
                </ul>
              </div>
            </div>
            <div role="flatdoc-content" class="content" id="content">
              {{#markdown}}
              {{> body}}
              {{/markdown}}
              <div class="code-samples"></div>
            </div>
          </div>
        </div>
        {{> footer}}
      </div>
    </div>

    <!-- Custom -->
    <script src="{{assets}}/js/docs.js"></script>
    <script src="{{assets}}/js/mobile-nav.js"></script>
  </body>
</html>
